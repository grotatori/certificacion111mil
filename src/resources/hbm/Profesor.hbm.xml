<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="centrocultural.model.Profesor" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="profesores">
      <id name="id" column="id" >
          <generator class="identity" />
      </id>
      <property name="apellido" type="string" column="apellido" />
      <property name="nombre" type="string" column="nombre" />
      <property name="telefono" type="long" column="telefono" />
      <property name="email" type="string" column="email" />
      <property name="sueldo" type="float" column="sueldo" />
  </class>
</hibernate-mapping>
